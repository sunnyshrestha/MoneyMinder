<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="moneyminder.suncha.com.moneyminder">

    <application
        android:name="com.orm.SugarApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartApplication">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ReceivableDetails" />

        <meta-data
            android:name="QUERY_LOG"
            android:value="true" />
        <meta-data
            android:name="DATABASE"
            android:value="MoneyMinderData.db" />
        <meta-data
            android:name="VERSION"
            android:value="1" />
        <meta-data
            android:name="DOMAIN_PACKAGE_NAME"
            android:value="moneyminder.suncha.com.moneyminder" />

        <activity android:name=".ViewAllReceivables" />
        <activity
            android:name=".popupReceivable"
            android:theme="@style/Theme.AppCompat.Light.Dialog.Alert"></activity>
    </application>

</manifest>